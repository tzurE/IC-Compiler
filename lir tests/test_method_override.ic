class A {
 int x;
 
 void f() {
	Library.print("A was called");
 }
}

class B extends A {
 int y;
 
 void f() {
	Library.print("B was called");
 }
 
 static void main(string[] args) {
	A a = new A();
	B b = new B();

	Library.print("Calling A, ");
	a.f();

	Library.print("; Calling B, ");
	b.f();
 }
}